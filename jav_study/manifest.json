{
  "name": "jav_study",
  "title": "学习资料",
  "author": "NaNaKo_",
  "description": "学习资料查询及下载。使用前务必查看README！",
  "_comment_start": "x-release-please-start-version",
  "version": "1.2.15",
  "_comment_end": "x-release-please-end",
  "requirements": [],
  "dependencies": {
    "appVersion": ">=1.9.47"
  },
  "configField": [
    {
      "fieldName": "uid",
      "fieldType": "Enum",
      "label": "推送用户",
      "helperText": "选择接收推送的用户,用户管理要独立设置推送配置才生效",
      "enumValuesRef": "UserList",
      "multiValue": true
    },
    {
      "fieldName": "ToChannelName",
      "fieldType": "Enum",
      "label": "推送通道名称",
      "helperText": "选择想要推送的通道,逗号间隔",
      "enumValuesRef": "ChannelNameList",
      "multiValue": true
    },
    {
      "fieldName": "proxy",
      "fieldType": "String",
      "label": "代理地址",
      "helperText": "网站访问需代理地址，可支持http代理和sock5代理。",
      "defaultValue": ""
    },
    {
      "fieldName": "user_agent",
      "fieldType": "String",
      "label": "访问网站的user-agent",
      "helperText": "访问网站的user-agent",
      "defaultValue": ""
    },
    {
      "fieldName": "smms_token",
      "fieldType": "String",
      "label": "smms网站的token",
      "helperText": "smms网站的token，用于上传老师头像。详情见https://sm.ms/home/apitoken",
      "defaultValue": ""
    },
    {
      "fieldName": "client_name",
      "fieldType": "Enum",
      "label": "配置的下载器名称",
      "helperText": "配置的下载器名称",
      "enumValuesRef": "DownloadClientList",
      "multiValue": false
    },
    {
      "fieldName": "down_path",
      "fieldType": "String",
      "label": "下载到下载器的路径",
      "helperText": "下载到下载器的路径",
      "defaultValue": ""
    },
    {
      "fieldName": "file_size_limit",
      "fieldType": "String",
      "label": "种子大小限制",
      "helperText": "限制下载的种子文件大小，填写两个数值，用英文逗号隔开，单位GB。如1,20表示1GB到20GB之间的种子文件。不填则不限制。",
      "defaultValue": ""
    },
    {
      "fieldName": "jav_list_enable",
      "fieldType": "Bool",
      "label": "是否自动下载榜单TOP20",
      "helperText": "是否自动下载榜单TOP20，勾选即自动下载并监控榜单更新。",
      "defaultValue": false
    },
    {
      "fieldName": "translate_enable",
      "fieldType": "Bool",
      "label": "是否使用百度翻译",
      "helperText": "是否使用百度翻译标题，需要填写appid和sercet，不勾选则不生效",
      "defaultValue": false
    },
    {
      "fieldName": "appid",
      "fieldType": "String",
      "label": "百度翻译接口appid",
      "helperText": "百度翻译接口appid，详情见http://api.fanyi.baidu.com/manage/developer",
      "defaultValue": ""
    },
    {
      "fieldName": "sercet",
      "fieldType": "String",
      "label": "百度翻译接口sercet",
      "helperText": "百度翻译接口sercet，详情见http://api.fanyi.baidu.com/manage/developer",
      "defaultValue": ""
    }
  ],
  "logoUrl": "/plugins/jav_study/logo.jpg",
  "payImageUrl": "https://p.xmoviebot.com/donate/chacha_wechat.jpg"
}
